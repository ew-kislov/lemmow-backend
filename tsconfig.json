{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "sourceMap": false,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": false,
    "plugins": [{"name": "typescript-tslint-plugin"}]
  },
  "exclude": [
    "node_modules",
    "dist",
    "test",
    "**/*test.ts"
  ]
}